<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                         "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	 <class name="com.iiitb.ooadvoid.pojo.FlopkartListingDeal" table="FlopkartListingDeal">
	 
      <meta attribute = "class-description">
         This class contains the Flopkart Listing Deal detail. 
      </meta>
      
      <id name = "id" type = "int" column = "id">
         <generator class="native"/>
      </id>
      
      <property name = "startdate" column = "startdate" type = "string"/>
      <property name = "enddate" column = "enddate" type = "string"/>
      <property name = "listingid" column = "listingid" type = "int" unique="true"/>
      <property name = "comboid" column = "comboid" type = "string"/>
      <property name = "comboDesc" column = "comboDesc" type = "string"/>
      <property name = "dealid" column = "dealid" type = "int" unique="true"/>
	 
	</class>
</hibernate-mapping>