<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	 <class name="com.iiitb.ooadvoid.pojo.FlopkartOrder" table="flopkartorder"  dynamic-update="true">
      
      <id name = "id" type = "int" column = "id">
         <generator class="native"/>
      </id>

      <property name = "UserId" column = "UserId" type = "int"/>
       <property name = "orderId" column = "OrderId" type = "int"/>
      <property name = "shippingAddress" column = "shippingAddress" type = "string"/>
      <property name = "itemId" column = "itemId" type = "string"/>
      <property name = "status" column = "status" type = "string"/>
	  <property name = "orderDate" column = "orderDate" type = "string"/>
	  <property name = "totalAmount" column = "totalAmount" type = "int"/>
	  <property name = "quantity" column = "quantity" type = "int"/>
	</class>
</hibernate-mapping>
